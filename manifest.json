{
  "manifest_version": 3,
  "name": "TabFeed",
  "version": "0.1.0",
  "permissions": [
    "activeTab",
    "contentSettings",
    "scripting",
    "storage",
    "system.memory",
    "tabGroups",
    "tabs",
    "webRequest",
    "history"
  ],
  "optional_host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "TabFeed",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "side_panel": {
    "default_path": "panel.html"
  }
}
